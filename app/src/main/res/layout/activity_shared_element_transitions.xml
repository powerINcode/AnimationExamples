<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.powerincode.screens.activityTransition.ActivityTransitions"
    tools:layout_editor_absoluteY="25dp">

    <ImageButton
        android:id="@+id/imageView2"
        android:layout_width="101dp"
        android:layout_height="97dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="41dp"
        android:onClick="goToActivity2"
        android:tag="ic_lock_lock"
        android:transitionName="@string/transition_shared_images"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_lock_lock" />

    <ImageButton
        android:id="@+id/imageView3"
        android:layout_width="114dp"
        android:layout_height="107dp"
        android:layout_marginBottom="204dp"
        android:layout_marginEnd="72dp"
        android:onClick="goToActivity2"
        android:tag="btn_star_big_on"
        android:transitionName="@string/transition_shared_images"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@android:drawable/btn_star_big_on" />
</androidx.constraintlayout.widget.ConstraintLayout>
